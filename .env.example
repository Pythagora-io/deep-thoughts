# Copy this file to .env and edit the settings

# Port to listen on (example: 3000)
PORT=

# MongoDB database URL (example: mongodb://localhost/dbname)
DATABASE_URL=

# Session secret string (must be unique to your server)
SESSION_SECRET=

# OpenAI API key (required for OpenAI integration)
OPENAI_API_KEY=your_openai_api_key_here # INPUT_REQUIRED {your OpenAI API key}

# Anthropic API key (required for Anthropic integration)
ANTHROPIC_API_KEY=your_anthropic_api_key_here # INPUT_REQUIRED {your Anthropic API key}